<% provide(:title, 'All users') %>

<div class="container">
  <h1 class="white-text">All users</h1>
  <%= will_paginate %>
  <div class=" card-3 bot-padding top-padding">
    <ul class="users">
    <% @users.each do |user| %>
      <li>
        <%= gravatar_for user, size: 50 %>
        <%= link_to user.name, user %>
      </li>
    <% end %>
  </ul>
  </div>
</div>

<%= will_paginate %>

